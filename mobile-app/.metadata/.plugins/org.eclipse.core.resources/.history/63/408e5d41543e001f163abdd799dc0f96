package com.shopping.ws.exception;

import java.time.LocalDateTime;
import java.util.Date;

public class MobileAppException {
	
	private LocalDateTime timeStamp;
	private String message;
	
	public LocalDateTime getTimeStamp() {
		return timeStamp;
	}
	public void setTimeStamp(LocalDateTime timeStamp) {
		this.timeStamp = timeStamp;
	}
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
	public MobileAppException() {
		super();
	}
	public MobileAppException(LocalDateTime timeStamp, String message) {
		super();
		this.timeStamp = timeStamp;
		this.message = message;
	}
	public MobileAppException(Date date, String localizedMessage) {
		// TODO Auto-generated constructor stub
	}
	
	

}
